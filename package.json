    "@babel/preset-env": "^7.0.0-beta.40",
    "babel-loader": "^8.0.0-beta.2",
    "ts-loader": "^4.0.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.7.0",
    "typedoc": "^0.9.0",
    "typedoc-plugin-markdown": "^1.0.12",
    "typescript": "^2.6.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9"
